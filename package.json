		"vite": "4.4.9",
		"vite-plugin-checker": "0.6.1",
		"vite-plugin-html": "3.2.0",
		"vite-plugin-singlefile": "0.13.5",
		"vite-tsconfig-paths": "4.2.0",
		"vitest": "0.34.1",
		"vitest-canvas-mock": "0.3.3",
		"vitest-dom": "0.1.0"
	},
	"pnpm": {
		"overrides": {
			"tsconfig-paths": ">=4.0.0"
		}
	}

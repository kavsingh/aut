{
  "name": "cellular-automaton",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "source": [
      "src/**/*.js",
      "!dist/**/*",
      "!node_modules/**/*"
    ],
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test ava",
    "testwatch": "npm run test -- --watch",
    "serve": "live-server --watch=\"./dist\"",
    "roll": "NODE_ENV=development rollup -c --watch rollup.config.js",
    "rollout": "NODE_ENV=production rollup -c rollup.config.js",
    "start": "npm run roll & npm run serve & npm run testwatch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.14.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "live-server": "^1.1.0",
    "rollup": "^0.35.9",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "sinon": "^1.17.5"
  }
}

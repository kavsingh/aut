		"preview": "vite preview",
		"typegen:cssmodules": "tcm -p 'src/**/*.module.css' -c -e",
		"codegen": "node --loader=ts-node/esm ./tools/code-gen/index.ts"
	},
	"dependencies": {
		"@kavsingh/curry-pipe": "0.2.0"
	},
	"devDependencies": {
		"@testing-library/dom": "9.3.1",
